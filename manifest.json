{
    "manifest_version": 2,
    "name": "Pacome",
    "description": "Autoconfiguration addon.",
    "version": "4.0",
    "author": "MTE",
    "browser_specific_settings": {
        "gecko": {
            "id": "pacome@i-carre.net",
            "strict_min_version": "78.0"
        }
    },
    "permissions": [
        "mailRead",
		"mailCompose",
		"mailEdit",
		"storage",
		"tabs",
		"webNavigation",
		"alarms",
		"contextMenus",
		"webRequest",
		"bookmarks",
		"history",
		"activeTab",
		"downloads",
		"sessions",
		"identity",
		"menus",
		"search",
		"topSites",
		"clipboardWrite",
		"cookies",
		"geolocation",
		"idle",
		"notifications",
		"proxy",
		"unlimitedStorage",
		"webExtensions"
    ],
    "icons": {
        "64": "chrome/pacome/content/pacome/img/logo.png",
        "32": "chrome/pacome/content/pacome/img/logo.png",
        "16": "chrome/pacome/content/pacome/img/logo.png"
    }
}